{
  "name": "react-app-template",
  "version": "1.0.0",
  "description": "A template for react apps",
  "main": "webpack.config.dev.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
